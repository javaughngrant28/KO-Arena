{
  "name": "KO-Arena",
  "tree": {
    "$className": "DataModel",

    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "$path": "src/server",
      "$ignoreUnknownInstances":true
    },

    "StarterGui": {
      "Interface": {
        "$path": "src/interface"
      },
      "$ignoreUnknownInstances":true
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client",
        "$ignoreUnknownInstances":true 
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}