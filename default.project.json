{
  "name": "KO-Arena",
  "tree": {
    "$className": "DataModel",

    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Shared": {
        "$className": "Folder",
        "$path": "src/Shared"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/Server"
    },

    "StarterGui":{
      "$className":"StarterGui",
      "Interface":{
        "$path": "src/Interface",
        "$className":"Folder"
      },
      "$ignoreUnknownInstances": true
    },
    
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "Client": {
          "$className": "Folder",
          "$path": "src/Client"
        }
      }
    },
    
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}